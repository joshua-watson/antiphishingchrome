{
    "manifest_version": 3,
    "name": "Phishing Detector",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Phishing Detector"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["model/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  